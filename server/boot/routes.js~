module.exports = function(app) {
  // Install a "/ping" route that returns "pong"
  app.get('/ping', function(req, res) {
    res.send('pong');
  });
}


module.exports = function(app) {
  // Install a "/ping" route that returns "pong"
  app.get('/students', function(req, res) {
    res.send('http://localhost:3000/api/students');
  });
}
